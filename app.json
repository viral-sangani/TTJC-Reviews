{
    "expo": {
        "name": "TTJC Review",
        "slug": "ttjc-reviews",
        "android": {
            "package": "com.viralsangani.ttjcreviews",
            "versionCode": 1
        },
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logo.png",
        "scheme": "myapp",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
